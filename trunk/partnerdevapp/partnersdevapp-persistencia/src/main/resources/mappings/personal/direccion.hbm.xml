<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.edu.unq.partnersdevapp.dominio.personal">
	<class name="Direccion" table="direccion" >
		<id name="id">
			<generator class="native" />
		</id>

		<property name="calle" />
		<property name="altura" />
		<property name="piso" />
		<property name="depto" />
		<property name="localidad" />
		<property name="codigoPostal" />
		<property name="provincia" />
		<property name="pais" />
			 	
	</class>	
		
</hibernate-mapping>