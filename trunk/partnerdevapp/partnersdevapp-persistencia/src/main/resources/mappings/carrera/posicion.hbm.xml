<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.edu.unq.partnersdevapp.dominio.carrera">

	<class name="Posicion" table="posicion">

		<id name="id">
			<generator class="native" />
		</id>
		
		<property name="numeroDeBanda" />		
			
		<many-to-one name="nivel" class="ar.edu.unq.partnersdevapp.dominio.carrera.Nivel" cascade="all"/>
	     
	</class>

</hibernate-mapping>
