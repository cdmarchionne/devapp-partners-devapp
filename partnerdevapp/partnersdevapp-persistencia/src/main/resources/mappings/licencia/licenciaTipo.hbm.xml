<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.edu.unq.partnersdevapp.dominio.licencia">

	<class name="LicenciaTipo" table="licenciaTipo">

		<id name="id">
			<generator class="native" />
		</id>

		<property name="tipo" column="tipo" />
		<property name="descripcion" column="descripcion" />
		<property name="remunerable" column="remunerable" />
		<property name="diasConsecutivos" column="diasConsecutivos" />
		<property name="diasCantidadAnuales" column="diasCantidadAnuales" />
		
	</class>

</hibernate-mapping>