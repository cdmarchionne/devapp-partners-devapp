<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ar.edu.unq.partnersdevapp.dominio.calendario">

	<class name="FechasXcomprension" table="tecnologia">

		<id name="id">
			<generator class="sequence" />
		</id>
		
		<property name="fechaInicio" column="fechaInicio" type="date" />
		
		<list name="dias" table="fxc_dias">
            <key column="id"/>
            <list-index column="orden"/>
            <element type="int" column="diaDeLaSemana"/>
        </list>
    
		<property name="fechaFin" column="fechaFin" type="date" />

		<property name="repeticiones" column="repeticiones" type="int" />

	</class>

</hibernate-mapping>